        var dt = new Date();
        var fdt = dt.getDate() + '/' + dt.getMonth() + '/' + dt.getFullYear();


        var today = new Date();
        var dd = today.getDate();
        var mm = today.getMonth() + 1; //January is 0!
        var minutesdata = today.getHours();
        var secondsdata = today.getSeconds();
        var yyyy = today.getFullYear();
        if (dd < 10) {
            dd = '0' + dd;
        }
        if (mm < 10) {
            mm = '0' + mm;
        }
        return dd + '/' + mm + '/' + yyyy;
    }
